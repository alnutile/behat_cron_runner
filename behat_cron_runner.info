name = Behat Cron Runner
description = Runs during cron to test @crtical tests
package = Behat Editor
core = 7.x

configure = admin/config/behat/settings
dependencies[] = composer_manager
dependencies[] = ctools
dependencies[] = dblog
dependencies[] = strongarm
dependencies[] = ultimate_cron
dependencies[] = views
dependencies[] = views_queue
dependencies[] = views_ui
features[ctools][] = strongarm:strongarm:1
features[ctools][] = views:views_default:3.0
features[features_api][] = api:2
features[variable][] = behat_cron_runner_profile
features[variable][] = behat_cron_runner_q_limit
features[variable][] = behat_cron_runner_thread_limit
features[variable][] = composer_manager_autobuild_file
features[variable][] = composer_manager_autobuild_packages
features[variable][] = composer_manager_file_dir
features[variable][] = composer_manager_vendor_dir
features[variable][] = default_ultimate_cron_job__behat_cron_runner_run
features[variable][] = file_public_path
features[views_view][] = queue
